{
  "name": "ember-lodash-shim",
  "version": "2.0.6",
  "description": "Lodash for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "start": "ember server",
    "test": "npm run lint && npm run test-addon",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-lodash-shim.git"
  },
  "homepage": "https://github.com/ciena-blueplanet/ember-lodash-shim",
  "engines": {
    "node": ">= 6.0.0"
  },
  "bugs": {
    "url": "https://github.com/ciena-blueplanet/ember-lodash-shim/issues"
  },
  "author": "Matthew Dahl (https://github.com/sandersky)",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "2.12.3",
    "ember-cli-chai": "^0.3.2",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-htmlbars-inline-precompile": "0.3.12",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.13.2",
    "ember-cli-shims": "^1.0.2",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.6.0",
    "ember-resolver": "^2.0.3",
    "ember-source": "~2.12.0",
    "ember-test-utils": "^1.10.3",
    "loader.js": "^4.2.3"
  },
  "keywords": [
    "ember-addon",
    "lodash",
    "lodash-es",
    "underscore",
    "utility"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.7",
    "ember-cli-htmlbars": "^1.1.1"
  },
  "ember-addon": {
    "after": [
      "ember-lodash"
    ],
    "configPath": "tests/dummy/config"
  }
}
