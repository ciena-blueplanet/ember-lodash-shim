{
  "name": "ember-lodash-shim",
  "version": "3.0.1",
  "description": "Lodash for Ember apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "lint-all-the-things",
    "lint:js": "eslint ./*.js addon addon-test-support app blueprints config lib server test-support tests",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "test-addon": "EMBER_TRY_SCENARIO=${EMBER_TRY_SCENARIO:=ember-default} && ember try:one $EMBER_TRY_SCENARIO --- COVERAGE=true ember test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ciena-blueplanet/ember-lodash-shim.git"
  },
  "homepage": "https://github.com/ciena-blueplanet/ember-lodash-shim",
  "engines": {
    "node": ">= 6.0.0"
  },
  "bugs": {
    "url": "https://github.com/ciena-blueplanet/ember-lodash-shim/issues"
  },
  "author": "Matthew Dahl (https://github.com/sandersky)",
  "license": "MIT",
  "devDependencies": {
    "bower": "^1.8.2",
    "broccoli-asset-rev": "^2.4.5",
    "chai-jquery": "^2.0.0",
    "ember-cli": "^3.1.4",
    "ember-cli-chai": "0.4.3",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mocha": "0.14.4",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-frost-test": "^4.0.1",
    "ember-hook": "1.4.2",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^0.7.0",
    "ember-source": "~3.1.1",
    "ember-source-channel-url": "^1.0.1",
    "ember-test-utils": "^8.1.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "^4.2.3",
    "sinon-chai": "^2.14.0"
  },
  "keywords": [
    "ember-addon",
    "lodash",
    "lodash-es",
    "underscore",
    "utility"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.8.2"
  },
  "ember-addon": {
    "after": [
      "ember-lodash"
    ],
    "configPath": "tests/dummy/config"
  }
}
